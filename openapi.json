  "components": {
    "schemas": {
      "AddFilterTransaction": {
        "type": "object",
        "required": [
          "commodityUnit",
          "code",
          "relatedId",
          "debitAccount",
          "creditAccountsFilter",
          "amount"
        ],
        "properties": {
          "amount": {
            "type": "integer",
            "format": "int64",
            "description": "amount added to debit account"
          },
          "code": {
            "type": "integer",
            "format": "int32",
            "description": "transaction code"
          },
          "commodityUnit": {
            "type": "string",
            "description": "commodity used"
          },
          "creditAccountsFilter": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "account name"
          },
          "debitAccount": {
            "type": "string",
            "description": "account name"
          },
          "relatedId": {
            "type": "string",
            "description": "random hex u128 string"
          }
        }
      },
      "AddFilterTransactions": {
        "type": "object",
        "required": [
          "fullDate2",
          "filterTransactions"
        ],
        "properties": {
          "filterTransactions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AddFilterTransaction"
            },
            "description": "list of transactions"
          },
          "fullDate2": {
            "type": "integer",
            "format": "int64",
            "description": "unix time milliseconds"
          }
        }
      },
      "AddTransaction": {
        "type": "object",
        "required": [
          "commodityUnit",
          "code",
          "relatedId",
          "debitAccount",
          "creditAccount",
          "amount"
        ],
        "properties": {
          "amount": {
            "type": "integer",
            "format": "int64",
            "description": "amount added to debit account"
          },
          "code": {
            "type": "integer",
            "format": "int32",
            "description": "transaction code"
          },
          "commodityUnit": {
            "type": "string",
            "description": "commodity used"
          },
          "creditAccount": {
            "type": "string",
            "description": "account name"
          },
          "debitAccount": {
            "type": "string",
            "description": "account name"
          },
          "relatedId": {
            "type": "string",
            "description": "random hex u128 string"
          }
        }
      },
      "AddTransactions": {
        "type": "object",
        "required": [
          "fullDate2",
          "transactions"
        ],
        "properties": {
          "fullDate2": {
            "type": "integer",
            "format": "int64",
            "description": "unix time milliseconds"
          },
          "transactions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AddTransaction"
            },
            "description": "list of transactions"
          }
        }
      },
      "Balance": {
        "type": "object",
        "required": [
          "accountName",
          "amount",
          "commodityUnit",
          "commodityDecimal"
        ],
        "properties": {
          "accountName": {
            "type": "string"
          },
          "amount": {
            "type": "integer",
            "format": "int64"
          },
          "commodityDecimal": {
            "type": "integer",
            "format": "int32"
          },
          "commodityUnit": {
            "type": "string"
          }
        }
      },
      "IncomeStatement": {
        "type": "object",
        "required": [
          "accountName",
          "amounts",
          "commodityUnit",
          "commodityDecimal"
        ],
        "properties": {
          "accountName": {
            "type": "string"
          },
          "amounts": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int64"
            }
          },
          "commodityDecimal": {
            "type": "integer",
            "format": "int32"
          },
          "commodityUnit": {
            "type": "string"
          }
        }
      },
      "QueryAccountBalancesBody": {
        "type": "object",
        "required": [
          "accounts_glob"
        ],
        "properties": {
          "accounts_glob": {
            "type": "string"
          },
          "date": {
            "type": [
              "integer",
              "null"
            ],
            "minimum": 0
          }
        }
      },
      "QueryAccountIncomeStatementBody": {
        "type": "object",
        "required": [
          "dates",
          "accounts_glob"
        ],
        "properties": {
          "accounts_glob": {
            "type": "string"
          },
          "dates": {
            "type": "array",
            "items": {
              "type": "integer",
              "minimum": 0
            }
          }
        }
      },
      "QueryTransactionsBody": {
        "type": "object",
        "required": [
          "date_newest",
          "date_oldest",
          "accounts_glob"
        ],
        "properties": {
          "accounts_glob": {
            "type": "string"
          },
          "date_newest": {
            "type": "integer",
            "minimum": 0
          },
          "date_oldest": {
            "type": "integer",
            "minimum": 0
          }
        }
      },
      "ResponseIncomeStatements": {
        "type": "object",
        "required": [
          "dates",
          "incomeStatements"
        ],
        "properties": {
          "dates": {
            "type": "array",
            "items": {
              "type": "integer",
              "minimum": 0
            }
          },
          "incomeStatements": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/IncomeStatement"
            }
          }
        }
      },
      "Transaction": {
        "type": "object",
        "required": [
          "commodityUnit",
          "commodityDecimal",
          "code",
          "fullDate",
          "fullDate2",
          "relatedId",
          "transferId",
          "debitAccount",
          "creditAccount",
          "debitAmount",
          "creditAmount"
        ],
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32",
            "description": "transaction code"
          },
          "commodityDecimal": {
            "type": "integer",
            "format": "int32",
            "description": "location of decimal point"
          },
          "commodityUnit": {
            "type": "string",
            "description": "commodity used"
          },
          "creditAccount": {
            "type": "string",
            "description": "account name"
          },
          "creditAmount": {
            "type": "integer",
            "format": "int64",
            "description": "amount removed from credit account"
          },
          "debitAccount": {
            "type": "string",
            "description": "account name"
          },
          "debitAmount": {
            "type": "integer",
            "format": "int64",
            "description": "amount added to debit account"
          },
          "fullDate": {
            "type": "integer",
            "format": "int64",
            "description": "unix time milliseconds"
          },
          "fullDate2": {
            "type": "integer",
            "format": "int64",
            "description": "unit time milliseconds"
          },
          "relatedId": {
            "type": "string",
            "description": "random hex u128 string"
          },
          "transferId": {
            "type": "string",
            "description": "random hex u128 string"
          }
        }
      },
      "Vec": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    }
  }
}